@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Required dependencies
find_dependency(Eigen3 3.4 REQUIRED)

# Optional dependencies based on build configuration
set(OPTMATHKERNELS_VULKAN_ENABLED @ENABLE_VULKAN@)
set(OPTMATHKERNELS_CUDA_ENABLED @ENABLE_CUDA@)
set(OPTMATHKERNELS_NEON_ENABLED @ENABLE_NEON@)

if(OPTMATHKERNELS_VULKAN_ENABLED)
    find_dependency(Vulkan)
endif()

if(OPTMATHKERNELS_CUDA_ENABLED)
    find_dependency(CUDAToolkit)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/OptMathKernelsTargets.cmake")

check_required_components(OptMathKernels)
